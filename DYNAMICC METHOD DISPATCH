class A {
    void display(){
      System.out.println("value of x in grand parent class");
    }
}

class B extends A {
 void display(){
      System.out.println("value of y in  parent class");
    }
    
}
class C extends A{
 void display(){
      System.out.println("value of x in child class");
    }
    
}

public class MethodAccess {
    public static void main(String[] args) {
        A obj = new C();
        obj.display();
       
    }
}
