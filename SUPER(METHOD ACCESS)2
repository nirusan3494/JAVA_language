class A {
    void display(){
      System.out.println("hello from parent class");
    }
}

class B extends A {
   void display(){
      System.out.println("hello from child class");
    } 
    void show(){
        super.display();
        display();
        
    }
}

public class MethodAccess {
    public static void main(String[] args) {
        B obj = new B();
        obj.show();
    }
}
