class A {
    int a = 10;
}

class B extends A {
    int a = 20;
    void show() {
        System.out.println("parent a: " + super.a);
        System.out.println("child a: " + a);
    }
}

public class variableAccess {
    public static void main(String[] args) {
        B obj = new B();
        obj.show();
    }
}
